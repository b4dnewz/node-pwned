{
  "name": "pwned-api",
  "version": "0.0.5",
  "description": "A very simple javascript client for HaveIBeenPwned API.",
  "homepage": "https://github.com/b4dnewz/node-pwned",
  "author": {
    "name": "b4dnewz",
    "email": "filippo@codekraft.it",
    "url": "https://b4dnewz.github.io/"
  },
  "files": ["lib"],
  "main": "dist/pwned.node.js",
  "bin": {
    "pwned": "./lib/cli.js"
  },
  "keywords": ["haveibeenpwned", "javascript-client"],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.22.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "lint-staged": "^7.0.5",
    "nock": "^9.2.5",
    "nsp": "^3.2.1",
    "prettier": "^1.12.1",
    "webpack-cli": "^2.1.2"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest --coverage",
    "build": "webpack"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "b4dnewz/node-pwned",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "commander": "^2.15.1",
    "node-fetch": "^2.1.2"
  }
}
